<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="title_bar_style">@style/TitleBarLightStyle</item>
    </style>

    <style name="abc">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="title_bar_divider_color">#DEE0E2</item>
        <item name="title_bar_divider_height">0.5dp</item>
        <item name="title_bar_bg_color">@android:color/black</item>
        <item name="title_bar_left_drawable">@drawable/icon_title_bar_back</item>
        <item name="title_bar_text_size">17dp</item>
        <item name="title_bar_text_color">@color/title_bar_back_color</item>
        <item name="title_bar_sub_text_color">#535353</item>
        <item name="title_bar_sub_text_size">13dp</item>
    </style>
</resources>
